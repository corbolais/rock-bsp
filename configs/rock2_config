#board name
BOARD=rock2
TYPECHIP=rk32

#configuration for u-boot
UBOOT_REPO="git@x1:/rockchip/rk/u-boot"
UBOOT_REV=rkproduct
UBOOT_DEFCONFIG=rk3288_defconfig
UBOOT=u-boot-rock2

#configuration for linux kernel
KERNEL_REPO="git://x1/rockchip/rk/kernel.git"
KERNEL_REV=radxa-rk3288-linux-dev
KERNEL_DEFCONFIG=rock2_defconfig
KERNEL=linux-rock2
KERNEL_TARGET=rk3288-tb.img

#initrd
INITRD_REPO="https://github.com/radxa/initrd.git"

#parameter
PARAMETER="parameter"

#package-file
PACKAGE_FILE="package-file"

#rootfs URL
ROOTFS_URL="http://releases.linaro.org/14.10/ubuntu/trusty-images/alip/linaro-trusty-alip-20141024-684.tar.gz"

#linux-boot.img args
BOOTIMG_TARGET=resource.img
