#board name
BOARD=rock_pro
TYPECHIP=rk31

#configuration for u-boot
UBOOT_REPO="git://x1/linux/u-boot-rockchip.git"
UBOOT_REV=u-boot-rk3188
UBOOT_DEFCONFIG=rk30xx
UBOOT_EXTRA=

#configuration for linux kernel
KERNEL_REPO="git://x1/linux/linux-rockchip.git"
KERNEL_REV=radxa-stable-3.0
KERNEL_DEFCONFIG=radxa_rock_linux_defconfig
KERNEL_EXTRA=

#initrd
INITRD_REPO="https://github.com/radxa/initrd.git"
INITRD_REV=master

#rootfs URL
ROOTFSEXT4_URL=
ROOTFS_URL="http://releases.linaro.org/14.10/ubuntu/trusty-images/alip/linaro-trusty-alip-20141024-684.tar.gz"

#linux-boot.img args
BOOTIMG_SECOND="/dev/null"
